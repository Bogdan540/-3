#include <iostream>
#include <vector>

using namespace std;

int main() {
    int p;
    cin >> p;

    if (p == 1) {
        cout << 2 << endl; // тільки "5" і "9"
        return 0;
    }
    
    if (p == 2) {
        cout << 4 << endl; // "55", "59", "95", "99"
        return 0;
    }

    // dp[розряд][0] - кількість чисел, що закінчуються на одну цифру 5
    // dp[розряд][1] - кількість чисел, що закінчуються на дві цифри 55
    // dp[розряд][2] - кількість чисел, що закінчуються на одну цифру 9
    // dp[розряд][3] - кількість чисел, що закінчуються на дві цифри 99

    vector<vector<long long>> dp(p + 1, vector<long long>(4, 0));

    dp[1][0] = 1; // "5"
    dp[1][2] = 1; // "9"

    for (int i = 2; i <= p; ++i) {
        dp[i][0] = dp[i-1][2] + dp[i-1][3]; // "5" може бути після "9" або "99"
        dp[i][1] = dp[i-1][0]; // "55" може бути після "5"
        dp[i][2] = dp[i-1][0] + dp[i-1][1]; // "9" може бути після "5" або "55"
        dp[i][3] = dp[i-1][2]; // "99" може бути після "9"
    }

    long long result = dp[p][0] + dp[p][1] + dp[p][2] + dp[p][3];
    cout << result << endl;

    return 0;
}
